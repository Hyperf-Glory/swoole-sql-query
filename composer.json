{
    "name": "i-nocturne/Swoole-Sql-Query",
    "type": "library",
    "description": "Object-oriented query builders for MySQL, Postgres, SQLite, and SQLServer; can be used with any database connection library.",
    "keywords": [
        "mysql",
        "pdo",
        "pgsql",
        "postgres",
        "postgresql",
        "sqlite",
        "sql server",
        "sqlserver",
        "query",
        "select",
        "insert",
        "update",
        "delete",
        "db",
        "database",
        "sql",
        "dml"
    ],
    "homepage": "https://github.com/i-nocturne/Swoole-Sql-Query",
    "authors": [
        {
            "name": "He Peter",
            "homepage": "https://github.com/i-nocturne/Swoole-Sql-Query"
        }
    ],
    "require": {
        "php": ">=5.3.9"
    },
    "autoload": {
        "psr-4": {
            "Swoole\\SqlQuery\\": "src/"
        }
    },
    "extra": {
        "aura": {
            "type": "library"
        }
    },
    "suggest": {
        "aura/sql": "Provides an extension to the native PDO along with a profiler and connection locator. Use version 2.*."
    }
}
